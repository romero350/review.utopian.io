			overflow: hidden;
			transition: all 1s ease;
		}

		/* Style the links inside the navigation bar */

		.topnav a {
			float: left;
			display: block;
			color: #f2f2f2;
			text-align: center;
			padding: 14px 16px;
			text-decoration: none;
			transition: all 1s ease;
		}

		.topnav ul li a {
			margin-top: -15px;
			display: inline-block;
			transition: all 1s ease;
		}

		.topnav a:hover {}

		.topnav .icon {
			display: none;
		}

		@media screen and (max-width: 890px) {
			.topnav ul li a {
				display: none;
				transition: all 1s ease;
			}
			.topnav a:nth-child(2) {
				display: block;
			}
			.topnav a.icon {
				float: right;
				margin-top: -12px;
				display: block;
			}
		}

		@media screen and (max-width: 890px) {
			.topnav.responsive ul {
				/* position: relative; */
				display: flex;
				flex-direction: column-reverse;
			}

			.topnav ul li {
				margin: 0 auto;
				transition: all 1s ease;
			}

			.topnav .responsive ul li {
				display: block;
			}

			.topnav .responsive ul li a {
				text-align: center;
			}

			.topnav.responsive .icon {
				position: absolute;
				right: 0;
				top: 0;
				margin-top: 5px;
			}
			.topnav.responsive a {
				float: none;
				display: block;
				text-align: left;
			}
			.topnav.responsive a b {
				padding-left: 15px;
			}

			.topnav .responsive a {
				float: none;
			}
		}

		@media screen and (max-width: 600px) {
			.footer-cols {
				display: flex;
				flex-direction: column;
				width: 100%;
				justify-content: center;
				margin: 0 auto;
			}

			.three-cols div {
				text-align: left;
				margin: 0 auto;
				justify-content: center;
			}

			.three-cols .ml-auto {
				text-align: center;
			}

			.midfoot .nav {
				margin: 0 auto;
				justify-content: center;
				text-align: center;
			}
		}

		.footerlink {
			color: #7583a5 !important;
			font-size: 16px;
			position: relative;
			text-decoration: none;
		}

		.footerlink:hover {
			color: #fff;
		}

		.footerlink:before {
			content: "";
			position: absolute;
			width: 100%;
			height: 1px;
			bottom: 0;
			left: 0;
			background-color: #fff;
			visibility: hidden;
			-webkit-transform: scaleX(0);
			transform: scaleX(0);
			-webkit-transition: all 0.3s ease-in-out 0s;
			transition: all 0.3s ease-in-out 0s;
		}

		.footerlink:hover:before {
			visibility: visible;
			-webkit-transform: scaleX(1);
			transform: scaleX(1);
		}
